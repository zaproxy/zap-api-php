{
    "name": "zaproxy/api-client",
    "description": "PHP Client API For OWASP ZAP",
    "keywords": ["ZAP"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "ZAP Development Team",
            "email": "zaproxy-develop@googlegroups.com",
            "homepage": "https://www.zaproxy.org/"
        },
        {
            "name": "Astra Security",
            "email": "opensource@getastra.com",
            "homepage": "https://www.getastra.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zap\\": "src/Zap/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zap\\": "tests/Zap/"
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-json": "^8.0",
        "ext-curl": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.85",
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse -l 2 src tests",
        "tests": "vendor/bin/phpunit tests"
    }
}
